{
  "$schema": "./node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:8888/v3/api-docs",
  "output": "src/app/api",
  "ignoreUnusedModels": false,
  "removeStrictNullChecks": false,
  "defaultTag": "Api",
  "serviceSuffix": "Service",
  "modelSuffix": "",
  "enumSuffix": "",
  "stripReadOnly": false,
  "stripWriteOnly": false,
  "stripUnusedModels": false,
  "stripUnusedTags": false,
  "stripUnusedOperations": false,
  "stripUnusedComponents": false,
  "stripUnusedSchemas": false,
  "stripUnusedParameters": false,
  "stripUnusedResponses": false,
  "stripUnusedRequestBodies": false,
  "stripUnusedHeaders": false,
  "stripUnusedSecuritySchemes": false,
  "stripUnusedCallbacks": false,
  "stripUnusedLinks": false,
  "stripUnusedExamples": false,
  "stripUnusedContent": false,
  "responseType": "json"
} 